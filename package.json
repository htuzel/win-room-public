{
  "name": "win-room",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 next dev",
    "dev:worker": "NODE_TLS_REJECT_UNAUTHORIZED=0 tsx watch services/poller/worker.ts",
    "dev:socket": "NODE_TLS_REJECT_UNAUTHORIZED=0 tsx watch services/socket/server.ts",
    "build": "next build",
    "start": "next start",
    "start:worker": "tsx services/poller/worker.ts",
    "start:socket": "tsx services/socket/server.ts",
    "lint": "eslint",
    "db:migrate": "node scripts/db/migrate.js",
    "db:setup": "bash scripts/setup-database.sh",
    "db:setup-wr": "bash scripts/setup-wr-schema.sh",
    "admin:create": "tsx scripts/create-admin.ts",
    "admin:create:quick": "tsx scripts/create-admin.ts --quick",
    "admin:setup": "bash scripts/quick-setup.sh",
    "hash-password": "node scripts/hash-password.js"
