    "howler": "^2.2.4",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "next": "14.2.7",
    "pg": "^8.11.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "zod": "^3.22.4",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/node": "^20",
    "@types/bcryptjs": "^2.4.6",
    "@types/canvas-confetti": "^1.9.0",
    "@types/howler": "^2.2.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.11.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "tsx": "^4.7.0"
  }
